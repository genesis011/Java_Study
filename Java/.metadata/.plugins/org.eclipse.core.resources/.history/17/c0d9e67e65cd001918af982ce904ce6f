package a9_19Player;

public interface controllable {
	void play();
	void stop();
}
public class Test{
	public static void main(String[] arg) {
		controllable controler =new controllable() {
			
			@Override
			public void stop() {
				System.out.println("PLAY");
			}
			
			@Override
			public void play() {
				System.out.println("STOP");
			}
		};
		controler.play();
		controler.stop();
	}
}
