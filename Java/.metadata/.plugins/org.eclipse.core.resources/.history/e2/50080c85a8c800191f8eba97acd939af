package student;

public class Student {

	public Student(int number, String name) {
		super();
		this.number=number;
		this.name=name;
	}
	private int number;
	private String name;
}
public class GraduateStudent extends Student {

	public GraduateStudent(int number, String name) {
		super(number, name);

	}
	
}
