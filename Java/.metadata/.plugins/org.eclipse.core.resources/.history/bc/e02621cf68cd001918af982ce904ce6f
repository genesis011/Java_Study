package a9_17Movable;

public class MovableTest {

	public static void main(String[] args) {
		Shape[] arrayOfShapes	=new Shape[3];
	
	int num1=(int)(Math.random()*46+1);
	int num2=(int)(Math.random()*46+1);
	
	for(int i=0;i<arrayOfShapes.length;i++) {
		arrayOfShapes[0]=(Shape) new Rectangle();
		arrayOfShapes[1]=(Shape) new Triangle();
		arrayOfShapes[2]=(Shape) new Circle();
		
		
		
		arrayOfShapes[i].move(num1, num2);
	}
	}
}
